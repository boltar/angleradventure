; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Angler Adventures"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Events="*res://scripts/events.gd"
Tiles="*res://scripts/tiles.gd"
Audio="*res://scripts/audio.gd"
Enums="*res://scripts/enums.gd"
Globals="*res://scripts/globals.gd"

[display]

window/size/viewport_width=2000
window/size/viewport_height=1500
window/size/initial_position_type=0
window/size/initial_position=Vector2i(500, -500)

[rendering]

renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
